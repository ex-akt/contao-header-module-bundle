{
    "name": "ex-akt/contao-header-module-bundle",
    "type": "contao-bundle",
    "description": "Set header layouts and include other modules",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Samuel Heer",
            "homepage": "https://github.com/samuelheer"
        }
    ],
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require": {
        "contao/core-bundle": "^4.9",
        "ex-akt/contao-style-bricks-bundle": "^0.2"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "extra": {
        "contao-manager-plugin": "ExAkt\\ContaoHeaderModuleBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "ExAkt\\ContaoHeaderModuleBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExAkt\\ContaoHeaderModuleBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/ex-akt/contao-header-module-bundle/issues",
        "source": "https://github.com/ex-akt/contao-header-module-bundle"
    }
}
